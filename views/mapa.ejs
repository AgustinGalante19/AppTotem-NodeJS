<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="css/styles.css">
    <link rel="icon" type="img/png" href="img/gchulogo.png">
    <title>Mapa</title>
</head>

<body class="landing-page">
    <!--Navbar-->
    <nav class="navbar navbar-expand-lg navbar-light" style="background-color: #007CFF; margin-bottom: 15px;">
        <div class="container-fluid">
            <a class="navbar-brand text-white" href="/">
                <img src="img/gchulogo.png" width="18%" alt="">
                Inicio
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active text-white" aria-current="page" href="#">Ingresar a GualeActiva</a>
                    </li>
                </ul>
                <div class="position-absolute top-50 end-0 translate-middle-y">
                    <h4 id="hora-navbar"></h4>
                </div>
            </div>
        </div>
    </nav>
    <!--Mapa-->
    <div class="container">
        <a class="btn btn-primary" id="btn-atras" type="button" href="/" style="margin: 10px">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left"
                viewBox="0 0 16 16">
                <path fill-rule="evenodd"
                    d="M15 8a.5.5 0 0 0-.5-.5H2.707l3.147-3.146a.5.5 0 1 0-.708-.708l-4 4a.5.5 0 0 0 0 .708l4 4a.5.5 0 0 0 .708-.708L2.707 8.5H14.5A.5.5 0 0 0 15 8z" />
            </svg>
            Volver</a>
        <div id="map"></div>
    </div>

    <!--boostrap css import-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
        crossorigin="anonymous"></script>
    <!--Jquery import-->
    <script type="text/javascript" src="js/jquery-3.6.0.min.js"></script>
    <script src="js/app.js"></script>
    <script>
        window.onload = hora_actual(), $('#onload').fadeOut()
        $(document).ready(function () {
            let btn_atras = document.getElementById("btn-atras");
            btn_atras.focus();
        });
    </script>
    <!--Api google maps-->
    <script async
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBH3UJYBjdWqib5Z7XDeSorJTmYSKdGACs&callback=iniciarmapa">
        </script>
</body>

</html>